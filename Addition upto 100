package AG;

class W
{
	int i,sum=0;
	void add(int start,int end)
	{
		for(i=start;i<=end;i++)
		{
			sum=sum+i;
		}
		System.out.println("The sum from " +start+ " to " +end+ " is " +sum);
		sum=0;
	}
}
public class AG {

	public static void main(String[] args)
	{
		W obj = new W();
		obj.add(1,100);
		obj.add(1, 50);
		obj.add(50,100);
		obj.add(1,10);
		obj.add(1,1);
		// TODO Auto-generated method stub

	}

}
